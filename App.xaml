<Application x:Class="RealtorObjects.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RealtorObjects"
             xmlns:v="clr-namespace:RealtorObjects.View"
             xmlns:vm="clr-namespace:RealtorObjects.ViewModel">
    <!--StartupUri="View/MainWindow.xaml"-->
    <Application.Resources>
        <ResourceDictionary>
            <FontFamily x:Key="Rounds">pack://application:,,,/View/Font/#RoundsBlack</FontFamily>
            <FontFamily x:Key="Helvetica">pack://application:,,,/View/Font/#Helvetica</FontFamily>
            <FontFamily x:Key="VAGWorld">pack://application:,,,/View/Font/#VAG World</FontFamily>
            <FontFamily x:Key="AFuturaRound">pack://application:,,,/View/Font/#a_FuturaRound</FontFamily>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="View/Dictionaries/ColorsAndBrushes.xaml"/>
                <ResourceDictionary Source="View/Dictionaries/ControlStylesAndTemplates.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
