<Window x:Class="RealtorObjects.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RealtorObjects"
        xmlns:v="clr-namespace:RealtorObjects.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="750" Width="1300">
    <Window.Resources>
    </Window.Resources>
    <Grid Name="TestGrid">
        <TabControl>
            <TabItem>
                <TabItem.Header>
                    <TextBlock Text="Тестовая"/>
                </TabItem.Header>
                <TabItem.Content>
                    <StackPanel>
                        <Button
                        Command="{Binding TestCommand}"
                        Width="50"
                        Height="50"/>
                        <Button
                        Command="{Binding SecondTestCommand}"
                        Width="50"
                        Height="50"/>
                    </StackPanel>
                </TabItem.Content>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
